{
  "name": "SwitchNginx",
  "version": "1.0.0",
  "description": "A tool to change the local nginx configuration file.",
  "main": "electron/main.js",
  "author": "hauk0101",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --mode development --config './web/config/webpack.config.js'",
